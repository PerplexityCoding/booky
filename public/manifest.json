{
  "name": "Book Keeper",
  "version": "0.0.1",
  "version_name": "0.0.1 beta",
  "description": "Manage your bookmark differently",
  "permissions": ["activeTab", "tabs", "storage"],
  "page_action": {
    "default_icon": {
      "16": "assets/images/get_started16.png",
      "32": "assets/images/get_started32.png",
      "48": "assets/images/get_started48.png",
      "128": "assets/images/get_started128.png"
    }
  },
  "icons": {
    "16": "assets/images/get_started16.png",
    "32": "assets/images/get_started32.png",
    "48": "assets/images/get_started48.png",
    "128": "assets/images/get_started128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
